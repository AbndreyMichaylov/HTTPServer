  </ItemGroup>

  <ItemGroup>
    <Page Include="src\Examples\ExampleProject1\Pages\Index.html">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Page>
  </ItemGroup>

</Project>
